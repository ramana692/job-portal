{
  "version": 2,
  "builds": [
    {
      "src": "jobportfolio-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/jobportfolio-frontend/build/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://jobportfolio-api.onrender.com/api",
    "CI": "false"
  }
}
