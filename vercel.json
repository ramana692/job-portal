{
  "buildCommand": "npm --prefix jobportfolio-frontend install && npm --prefix jobportfolio-frontend run build",
  "outputDirectory": "jobportfolio-frontend/build",
  "installCommand": "npm --prefix jobportfolio-frontend install",
  "framework": null,
  "env": {
    "CI": "false"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
